{
  "name": "@auscope/angular-cesium",
  "version": "18.0.0",
  "license": "MIT",
  "description": "Angular v18 library for working with Cesium, fork of the ArticodeId library. (https://github.com/articodeltd/angular-cesium)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AuScope/angular-cesium.git"
  },
  "keywords": [
    "Angular",
    "Cesium",
    "Angular Cesium"
  ],
  "scripts": {
    "ng": "ng",
    "demo:start": "ng serve angular-cesium-demo",
    "demo:build": "ng build angular-cesium-demo --configuration production",
    "landing-page:deploy": "ng build landing-page --configuration production && cp projects/landing-page/now.json dist/landing-page && cd dist/landing-page && now --configuration production",
    "landing-page": "ng serve landing-page --host 0.0.0.0 --disableHostCheck",
    "lib:build": "ng build angular-cesium --configuration production && npm run copy:extra-files",
    "lib:build-orig": "ng build angular-cesium --configuration production && npm run copy:extra-files && npm run schemnantics:build",
    "lib:start": "ng build angular-cesium --watch",
    "lib:publish": "npm run lib:build && cd projects/angular-cesium/dist && npm publish",
    "lib:publish-orig": "npm run lib:build && cd dist/angular-cesium && npm publish && cd ../../ && npm run docs:push",
    "test": "ng test angular-cesium",
    "test:ci": "ng test angular-cesium --no-watch --browsers=ChromeHeadless",
    "lint": "npx eslint -c ./eslint.config.mjs ./projects/angular-cesium/src/lib/angular-cesium",
    "e2e": "ng e2e",
    "gh-pages": "gh-pages -d docs -b gh-pages",
    "copy:extra-files": "cp README.md LICENSE.txt projects/angular-cesium/dist/",
    "schemnantics:build": "cd projects/angular-cesium && yarn build",
    "docs:run": "compodoc -p projects/angular-cesium/src/lib/tsconfig.compodoc.json --theme stripe -d docs -n \"Angular Cesium\" --watch --serve",
    "docs:compile": "rimraf docs && compodoc -p projects/angular-cesium/src/lib/tsconfig.compodoc.json --theme stripe -d docs -n \"Angular Cesium\"",
    "docs:push": "npm run docs:compile && npm run gh-pages && rimraf docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.8",
    "@angular/cdk": "^18.2.9",
    "@angular/common": "^18.2.8",
    "@angular/compiler": "^18.2.8",
    "@angular/core": "^18.2.8",
    "@angular/forms": "^18.2.8",
    "@angular/material": "^18.2.9",
    "@angular/platform-browser": "^18.2.8",
    "@angular/platform-browser-dynamic": "^18.2.8",
    "@angular/router": "^18.2.8",
    "@auscope/angular2parse": "^18.0.1",
    "@eslint/compat": "^1.2.1",
    "@fortawesome/angular-fontawesome": "~0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@types/lodash": "^4.14.180",
    "cesium": "1.93.0",
    "core-js": "^3.24.1",
    "dataloader": "^1.4.0",
    "geodesy": "^1.1.3",
    "gh-pages": "^6.2.0",
    "heatmap.js": "^2.0.5",
    "json-string-mapper": "^1.0.0",
    "lodash": "^4.17.21",
    "primitive-primitives": "0.0.6",
    "rxjs": "~6.6.7",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.4.1",
    "when": "^3.7.8",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.9",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular/cli": "^18.2.9",
    "@angular/compiler-cli": "^18.2.8",
    "@angular/language-service": "^18.2.8",
    "@compodoc/compodoc": "^1.1.19",
    "@stylistic/eslint-plugin-js": "^2.9.0",
    "@types/body-parser": "^1.16.3",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.16.0",
    "@types/geodesy": "^1.1.3",
    "@types/jasmine": "~3.10.6",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^13.13.52",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "globals": "^15.11.0",
    "https-browserify": "^1.0.0",
    "jasmine-core": "~3.99.1",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^18.2.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "protractor": "~7.0.0",
    "rimraf": "^2.6.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^7.0.1",
    "typescript": "5.4.5",
    "url": "^0.11.0",
    "zlib": "^1.0.5"
  }
}
